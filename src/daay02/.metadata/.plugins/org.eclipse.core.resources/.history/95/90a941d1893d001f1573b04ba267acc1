package day12;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.HashSet;
import java.util.Iterator;
import java.util.LinkedList;
import java.util.List;
import java.util.Map;
import java.util.Scanner;
import java.util.Set;

public class TrtCatch {

	//EXCEPTION, THREAD, INNERCLASS
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//컬력션 프레임 워크
		//리스트, 셋, 맵
		
		//리스트
		//배열의 사용 편의를 위해 만들어잔 객체
		//동적배열 <메모리 크기가 변화된다
		//변화라는 단어는 삭제와 추가기능
		
		//어레이리스트보다 느릴수 있다
		//링크드 리스트와 어레이 리스트의 속도차이가 존재하므로
		//하나만 맹몾겆으로 사용하는 것은 지양
		
		//동적바인딩
		//상속
		//오버라이드 된 메서드만 사용가능
		List<String> list; // = new LinkedList<>();
		list = new ArrayList<>();
		
		list.add("슈퍼맨");
		list.add("배트맨");
		
		for(int i = 0; i< list.size(); i++) {
			System.out.println(i);
		}
		for(String li : list) {
			System.out.println(li);
		}
		
		Iterator<String> it = list.iterator();
		while(it.hasNext()) {
			String e = it.next();
			System.out.println(e);
		}
		
		//중복제거
		Set<String> set = new HashSet<String>();
		
		set.add("슈퍼맨");
		set.add("슈퍼맨");
		set.add("슈퍼맨"); //하나는 입력이 안된다
		
		System.out.println(set.size());
		
		//기본 for문은 사용할 수없다
		
		Iterator<String> it2 = set.iterator();
		while(it2.hasNext()) {
			String e = it2.next();
			System.out.println(e);
		}
		for(String li : set) {
			System.out.println(li);
		}
		//key value
		//set의 중복제거 기능을 하벼쳐서 만들어진 객체
		//key는 set의 중복제거를 가지고 옴
		//데이터 가공을 목적(자바 외에서 데이터를 가지고 올떄)
		Map<String, String> map = new HashMap<String, String>();
		
		map.put("num", "슈퍼맨");
		System.out.println("num");
		
		Set<String> keyList = map.keySet();
		for(String key : keyList) {
			System.out.println("Key: "+key);
//			System.out.println("value: "map.get(key));
		}
		
		//exception 예외처리
		Scanner sc = new Scanner(System.in);
		
		//Excpetion
		//프로그램 구동이 멈춤
		//개발자가 이미 인지하고 있는 에러를 예외처리 하는 것
		while(true) {
			try {
				int num = sc.nextInt();
				System.out.println("입력값: "+num);
			} catch (Exception e) {
				System.out.println("error");
			}
		}
		

	}

}
